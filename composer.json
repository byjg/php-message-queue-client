{
  "name": "byjg/message-queue-client",
  "description": "Minimal PHP foundation for building any message-queue driver.",
  "autoload": {
    "psr-4": {
      "ByJG\\MessageQueueClient\\": "src/"
    }
  },
  "autoload-dev": {
      "psr-4": {
        "Tests\\": "tests/"
      }
  },
  "prefer-stable": true,
  "minimum-stability": "dev",
  "require": {
    "php": ">=8.1 <8.5",
    "ext-curl": "*",
    "byjg/uri": "^6.0",
    "psr/log": "^1.0|^2.0|^3.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^10.5|^11.5",
    "vimeo/psalm": "^5.9|^6.2"
  },
  "scripts": {
    "test": "vendor/bin/phpunit",
    "psalm": "vendor/bin/psalm"
  },
  "license": "MIT"
}
